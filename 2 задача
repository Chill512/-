from math import*
t=float(input('ввудите :'))
a =2 * t
b=-(4 * t - 1 )
c = t + 3
if a==0:
    x = - c / b
    print('x' ,x)
else:
    D= b**2 - 4*a*c
    print ('D:', D)
    print ('a:', a)
    print ('b:', b)
    print ('c:', c)
    if D >= 0:
        x1= (- b + D **0.5) /(2 * a)
        x2= (- b - D ** 0.5) /(2 * a)
        print('xl=',x1,'x2',x2)
    else:
        print ('D < O' )
